@model Winnosuke.Models.Garage
<h2>Xác nhận xoá Garage</h2>
<dl class="row">
    <dt class="col-sm-2">Tên</dt>
    <dd class="col-sm-10">@Model.Name</dd>
    <dt class="col-sm-2">Địa chỉ</dt>
    <dd class="col-sm-10">@Model.Address</dd>
</dl>
<form asp-action="DeleteConfirmed">
    <input type="hidden" asp-for="GarageId" />
    <button type="submit" class="btn btn-danger">Xoá</button>
    <a asp-action="Index" class="btn btn-secondary">Huỷ</a>
</form>